#include <stdio.h>

int main()
{
 int arr[] ={12, 45, 2, 67, 33};
 int n= sizeof(arr)/ sizeof arr[0];
 int max =  arr[0];
 for(int i=1;i<n;i++)
 {
     if(arr[i]>max){
         max= arr[i];
     }
 }
 printf("Maximum element is: %d\n", max);
    return 0;
}


//python
arr=[12, 45, 2, 67, 33]
 max_num =  arr[0]
 
 for i in range (1, len(arr)):
     if arr[i] > max_num:
         max_num= arr[i]
         
 print("Maximum element is:", max_num )
