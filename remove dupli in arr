#include <stdio.h>

int main()
{
    int arr[]={1, 1, 2};
    int n= sizeof(arr)/ sizeof (arr[0]);
    int count=0;
    for(int i=0;i<n;i++){
        for(int j=i+1;j<n;j++)

    {
        if (arr[i]==arr[j]){
           count++;
        break; 
        }
        
    }
}
        printf("%d",count );
    
    return 0;
}

//pyhton
arr=[1, 1, 2]
     count = 0
     n = len(arr)
    for i in range(n):
        for j in range(i + 1, n):
            
        if (arr[i]==arr[j]):
           count+=1
        break
    
        print(count )
